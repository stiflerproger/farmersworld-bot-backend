{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "@providers/*": [
        "src/providers/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@modules/*": [
        "src/modules/*"
      ],
    }
  }
}
